{
  "name": "portfolio",
  "version": "4.0.0",
  "dependencies": {
    "@babel/core": "7.8.4",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "bluebird": "3.7.2",
    "breakpoint-sass": "2.7.1",
    "classnames": "2.2.6",
    "fetch-jsonp": "1.1.3",
    "gatsby": "2.19.24",
    "gatsby-plugin-google-analytics": "2.1.36",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-preact": "3.1.26",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sass": "2.1.29",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-twitter": "2.1.19",
    "gatsby-remark-copy-linked-files": "2.1.37",
    "gatsby-remark-images": "3.1.44",
    "gatsby-remark-prismjs": "3.3.31",
    "gatsby-remark-responsive-iframe": "2.2.32",
    "gatsby-remark-smartypants": "2.1.21",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-remark": "2.6.53",
    "gatsby-transformer-sharp": "2.3.16",
    "lodash": "4.17.15",
    "node-sass": "4.13.1",
    "normalize.css": "8.0.1",
    "preact": "10.3.3",
    "prismjs": "1.19.0",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-uid": "2.2.0",
    "to-sentence-case": "1.0.0",
    "twit": "2.2.11",
    "webpack": "4.42.0"
  },
  "devDependencies": {
    "@sitaggart/eslint-config": "0.5.45",
    "@sitaggart/prettier-config": "0.2.0",
    "bundlesize": "0.18.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "percy": "0.24.0",
    "precise-commits": "1.0.2",
    "prettier": "1.19.1"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "clean": "rm -rf public && rm -rf .cache",
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "dev": "gatsby develop -p $PORT",
    "build": "yarn clean && gatsby build --prefix-paths",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src/ *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundlesize": "bundlesize",
    "percy": "npx percy snapshot public/ --snapshot-files index.html,posts/index.html,2019-01-11-im-super-good-at-css-and-i-dont-recommend-the-cascade-dont-@-me/index.html",
    "now-build": "yarn build",
    "prettier": "prettier -l '**/*.{js,jsx}'",
    "prettier:fix": "prettier --write '**/*.{js,jsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "68 kB"
    },
    {
      "path": "./public/commons-*.js",
      "maxSize": "55 kB"
    },
    {
      "path": "./public/component-*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/webpack-*.js",
      "maxSize": "5 kB"
    }
  ]
}
