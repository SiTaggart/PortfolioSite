{
  "name": "portfolio",
  "version": "4.0.0",
  "dependencies": {
    "@babel/core": "7.9.6",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.1.0",
    "bluebird": "3.7.2",
    "breakpoint-sass": "2.7.1",
    "classnames": "2.2.6",
    "fetch-jsonp": "1.1.3",
    "gatsby": "2.21.24",
    "gatsby-plugin-google-analytics": "2.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-preact": "3.3.1",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sass": "2.3.1",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-twitter": "2.3.1",
    "gatsby-remark-copy-linked-files": "2.3.2",
    "gatsby-remark-images": "3.3.2",
    "gatsby-remark-prismjs": "3.5.1",
    "gatsby-remark-responsive-iframe": "2.4.2",
    "gatsby-remark-smartypants": "2.3.1",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "gatsby-transformer-sharp": "2.5.2",
    "lodash": "4.17.15",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "preact": "10.4.1",
    "prismjs": "1.20.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-uid": "2.2.0",
    "to-sentence-case": "1.0.0",
    "twit": "2.2.11",
    "webpack": "4.43.0"
  },
  "devDependencies": {
    "@sitaggart/eslint-config": "0.5.65",
    "@sitaggart/prettier-config": "0.2.0",
    "bundlesize": "0.18.0",
    "bundlewatch": "0.2.7",
    "eslint": "7.0.0",
    "husky": "4.2.5",
    "now": "19.0.0",
    "percy": "0.26.2",
    "precise-commits": "1.0.2",
    "prettier": "2.0.5"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "clean": "rm -rf public && rm -rf .cache",
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "dev": "gatsby develop -p $PORT",
    "build": "yarn clean && gatsby build --prefix-paths",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src/ *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundlesize": "bundlesize",
    "percy": "npx percy snapshot public/ --snapshot-files index.html,posts/index.html,2019-01-11-im-super-good-at-css-and-i-dont-recommend-the-cascade-dont-@-me/index.html",
    "now": "now",
    "now-build": "yarn build",
    "prettier": "prettier -l '**/*.{js,jsx}'",
    "prettier:fix": "prettier --write '**/*.{js,jsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "./public/app-*.js",
        "maxSize": "68 kB"
      },
      {
        "path": "./public/framework-*.js",
        "maxSize": "55 kB"
      },
      {
        "path": "./public/component-*.js",
        "maxSize": "10 kB"
      },
      {
        "path": "./public/styles-*.js",
        "maxSize": "5 kB"
      },
      {
        "path": "./public/styles.*.css",
        "maxSize": "5 kB"
      },
      {
        "path": "./public/webpack-*.js",
        "maxSize": "5 kB"
      }
    ]
  }
}
