{
  "name": "portfolio",
  "version": "5.0.0",
  "dependencies": {
    "@mdx-js/loader": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@next/mdx": "11.1.3",
    "@twilio-paste/core": "10.18.0",
    "@twilio-paste/icons": "5.9.0",
    "isomorphic-unfetch": "3.1.0",
    "lodash.reverse": "4.0.1",
    "lodash.sortby": "4.7.0",
    "mdx-prism": "0.3.4",
    "next": "11.1.3",
    "next-mdx-enhanced": "5.0.0",
    "next-seo": "4.28.1",
    "prismjs": "1.25.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-uid": "2.3.1",
    "swr": "1.1.0",
    "twit": "2.2.11",
    "twitter-text": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@emotion/babel-preset-css-prop": "10.2.1",
    "@next/eslint-plugin-next": "11.1.3",
    "@percy/cypress": "2.3.4",
    "@sitaggart/eslint-config-ts": "0.6.4",
    "@sitaggart/prettier-config": "0.3.3",
    "@types/lodash.reverse": "4.0.6",
    "@types/lodash.sortby": "4.7.6",
    "@types/mdx-js__react": "1.5.5",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/twit": "2.2.30",
    "@types/twitter-text": "3.1.1",
    "babel-plugin-import-glob-array": "0.2.0",
    "babel-plugin-inline-react-svg": "1.1.2",
    "bundlewatch": "0.3.2",
    "cypress": "8.7.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.3",
    "husky": "7.0.4",
    "jest": "27.4.3",
    "netlify-cli": "8.0.16",
    "percy": "0.28.6",
    "precise-commits": "1.0.2",
    "prettier": "2.5.1",
    "rehype": "12.0.0",
    "start-server-and-test": "1.14.0",
    "typescript": "4.5.2"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --format codeframe",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci:checks": "yarn build && yarn lint && yarn prettier && yarn bundlewatch && yarn cypress:ci",
    "cypress": "start-server-and-test start http://localhost:3000 cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "yarn percy:exec cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "percy": "percy",
    "percy:exec": "percy exec --",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "svg-to-react": "svg-to-react",
    "prepare": "husky install"
  },
  "browserslist": [
    "defaults"
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "./.next/static/chunks/**/*.js",
        "maxSize": "85 kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ],
      "repoBranchBase": "main"
    }
  }
}
