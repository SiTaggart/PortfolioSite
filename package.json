{
  "name": "portfolio",
  "version": "4.0.0",
  "dependencies": {
    "@babel/core": "7.5.5",
    "autoprefixer": "9.6.1",
    "babel-loader": "8.0.6",
    "breakpoint-sass": "2.7.1",
    "classnames": "2.2.6",
    "fetch-jsonp": "1.1.3",
    "gatsby": "2.13.79",
    "gatsby-plugin-google-analytics": "2.1.10",
    "gatsby-plugin-manifest": "2.2.9",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-preact": "3.1.6",
    "gatsby-plugin-react-helmet": "3.1.5",
    "gatsby-plugin-react-svg": "2.1.2",
    "gatsby-plugin-sass": "2.1.12",
    "gatsby-plugin-sharp": "2.2.17",
    "gatsby-plugin-twitter": "2.1.4",
    "gatsby-remark-copy-linked-files": "2.1.10",
    "gatsby-remark-images": "3.1.17",
    "gatsby-remark-prismjs": "3.3.9",
    "gatsby-remark-responsive-iframe": "2.2.8",
    "gatsby-remark-smartypants": "2.1.5",
    "gatsby-source-filesystem": "2.1.16",
    "gatsby-transformer-remark": "2.6.19",
    "gatsby-transformer-sharp": "2.2.11",
    "lodash": "4.17.15",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "preact": "10.0.0-rc.1",
    "prismjs": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "to-sentence-case": "1.0.0",
    "twit": "2.2.11",
    "webpack": "4.39.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "bundlesize": "0.18.0",
    "eslint": "6.2.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "husky": "3.0.4",
    "percy": "0.10.0",
    "precise-commits": "1.0.2",
    "prettier": "1.18.2"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "clean": "rm -rf public && rm -rf .cache",
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "yarn clean && gatsby build --prefix-paths",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src/ *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundlesize": "bundlesize",
    "percy": "npx percy snapshot public/ --snapshot-files index.html,posts/index.html,2019-01-11-im-super-good-at-css-and-i-dont-recommend-the-cascade-dont-@-me/index.html",
    "now-build": "yarn build",
    "now-dev": "yarn develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "68 kB"
    },
    {
      "path": "./public/1-*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/component-*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/webpack-*.js",
      "maxSize": "5 kB"
    }
  ]
}
