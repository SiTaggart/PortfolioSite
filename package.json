  "dependencies": {
    "@mdx-js/loader": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@next/mdx": "10.0.5",
    "@twilio-paste/core": "4.3.1",
    "@twilio-paste/icons": "3.12.1",
    "isomorphic-unfetch": "3.1.0",
    "lodash.reverse": "4.0.1",
    "lodash.sortby": "4.7.0",
    "mdx-prism": "0.3.1",
    "next": "10.0.5",
    "next-mdx-enhanced": "5.0.0",
    "next-seo": "4.17.0",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-uid": "2.3.1",
    "swr": "0.4.1",
    "twit": "2.2.11",
    "twitter-text": "3.1.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "10.2.1",
    "@percy/cypress": "2.3.4",
    "@sitaggart/eslint-config-ts": "0.5.257",
    "@sitaggart/prettier-config": "0.2.52",
    "@types/lodash.reverse": "4.0.6",
    "@types/lodash.sortby": "4.7.6",
    "@types/mdx-js__react": "1.5.3",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/twit": "2.2.28",
    "@types/twitter-text": "3.1.0",
    "babel-plugin-import-glob-array": "0.2.0",
    "babel-plugin-inline-react-svg": "1.1.2",
    "bundlewatch": "0.3.2",
    "cypress": "6.3.0",
    "eslint": "7.18.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "now": "21.0.1",
    "percy": "0.28.6",
    "precise-commits": "1.0.2",
    "prettier": "2.2.1",
    "rehype": "11.0.0",
    "start-server-and-test": "1.11.7",
    "typescript": "4.1.3"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "dev": "next",
    "build": "next build ; next build",
    "start": "next start",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --format codeframe",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress": "start-server-and-test start http://localhost:3000 cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "yarn percy:exec cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "percy": "percy",
    "percy:exec": "percy exec --",
    "now": "now",
    "now-build": "yarn build",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "svg-to-react": "svg-to-react"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "./.next/static/chunks/**/*.js",
        "maxSize": "85 kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ],
      "repoBranchBase": "main"
    }
  }
}
