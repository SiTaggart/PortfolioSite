{
  "name": "portfolio",
  "version": "4.0.0",
  "dependencies": {
    "autoprefixer": "9.5.1",
    "babel-loader": "8.0.6",
    "breakpoint-sass": "2.7.1",
    "classnames": "2.2.6",
    "fetch-jsonp": "1.1.3",
    "gatsby": "2.7.1",
    "gatsby-plugin-google-analytics": "2.0.20",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-preact": "3.0.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-react-svg": "2.1.1",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-plugin-sharp": "2.1.1",
    "gatsby-remark-copy-linked-files": "2.0.12",
    "gatsby-remark-images": "3.0.13",
    "gatsby-remark-prismjs": "3.2.9",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.37",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.19",
    "lodash": "4.17.11",
    "netlify-lambda": "1.4.13",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "prismjs": "1.16.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "to-sentence-case": "1.0.0",
    "twit": "2.2.11",
    "webpack": "4.32.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "bundlesize": "0.17.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.3.0",
    "precise-commits": "1.0.2",
    "prettier": "1.17.1"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "start": "gatsby develop",
    "start:functions": "netlify-lambda serve /src/functions",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && netlify-lambda build /src/functions",
    "build": "yarn clean && gatsby build",
    "build:functions": "netlify-lambda build /src/functions",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src/ *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "bundlesize": "bundlesize",
    "clean": "rm -rf public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "yarn pre-push"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "60 kB"
    },
    {
      "path": "./public/1-*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/component-*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/pages-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/styles-*.js",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/styles.*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./public/webpack-*.js",
      "maxSize": "5 kB"
    }
  ]
}
