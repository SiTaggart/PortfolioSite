  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@emotion/babel-preset-css-prop": "10.2.1",
    "@next/eslint-plugin-next": "12.1.5",
    "@percy/cypress": "2.3.4",
    "@sitaggart/eslint-config-ts": "0.6.10",
    "@sitaggart/prettier-config": "0.3.9",
    "@types/lodash.reverse": "4.0.6",
    "@types/lodash.sortby": "4.7.6",
    "@types/mdx-js__react": "1.5.5",
    "@types/node": "17.0.10",
    "@types/react": "17.0.44",
    "@types/twit": "2.2.30",
    "@types/twitter-text": "3.1.1",
    "babel-plugin-import-glob-array": "0.2.0",
    "babel-plugin-inline-react-svg": "2.0.1",
    "bundlewatch": "0.3.3",
    "cypress": "9.5.4",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-formatter-codeframe": "7.32.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "netlify-cli": "9.16.6",
    "percy": "0.28.7",
    "precise-commits": "1.0.2",
    "prettier": "2.6.2",
    "rehype": "12.0.1",
    "start-server-and-test": "1.14.0",
    "typescript": "4.6.3"
  },
  "homepage": "https://www.simontaggart.com",
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:SiTaggart/PortfolioSite.git"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --format codeframe",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci:checks": "yarn build && yarn lint && yarn prettier && yarn bundlewatch && yarn cypress:ci",
    "cypress": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress:open'",
    "cypress:open": "cypress open",
    "cypress:run": "yarn percy:exec cypress run",
    "cypress:ci": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress:run'",
    "percy": "percy",
    "percy:exec": "percy exec --",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "svg-to-react": "svg-to-react",
    "prepare": "husky install"
  },
  "browserslist": [
    "defaults"
  ],
  "bundlewatch": {
    "files": [
      {
        "path": "./.next/static/chunks/**/*.js",
        "maxSize": "85 kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ],
      "repoBranchBase": "main"
    }
  }
}
